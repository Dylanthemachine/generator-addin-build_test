{
  "name": "<%= title %>",
  "supportEmail": "<%= supportEmail %>",
  "version": "0.0.1",
  "items": [{
    <% if (isButton) { %>
    "page": "<%= page %>",
    "click": "<%= url %>",
    "buttonName": {
      "en": "<%= menuName %>"
    }
    <% } else { %>
    "url": "<%= url %>",
    "path": "<%= path %>",
    "icon": "images/icon.svg",
    "menuName": {
      "en": "<%= menuName %>"
    }
    <% } %>
  }],
  "dev": {
    "root": "<%= root %>",
    "dist": {
      "host": "<%= host %>"
    }
  }
}
